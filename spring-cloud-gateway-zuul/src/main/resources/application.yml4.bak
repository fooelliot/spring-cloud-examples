spring:
  application:
    name: springCloud-gateway-zuul

server:
  port: 8111
  
eureka: 
  client: 
    service-url: 
      defaultZone: http://user:password123@localhost:8761/eureka

zuul:
  routes:
    abc:
      path: /user/**
      serviceId: springcloud-provider

springcloud-provider:
  ribbon:
    listOfServers: http://localhost:8001,http://localhost:8002

ribbon:
  eureka:
    enabled: false